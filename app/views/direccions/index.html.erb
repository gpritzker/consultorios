<p id="notice"><%= notice %></p>

<h1>Listing Direccions</h1>

<table>
  <thead>
    <tr>
      <th>Calle</th>
      <th>Localidad</th>
      <th>Nro</th>
      <th>Piso</th>
      <th>Entrecalles</th>
      <th>Manzana</th>
      <th>Lote</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @direccions.each do |direccion| %>
      <tr>
        <td><%= direccion.calle %></td>
        <td><%= direccion.localidad %></td>
        <td><%= direccion.nro %></td>
        <td><%= direccion.piso %></td>
        <td><%= direccion.entreCalles %></td>
        <td><%= direccion.manzana %></td>
        <td><%= direccion.lote %></td>
        <td><%= link_to 'Show', direccion %></td>
        <td><%= link_to 'Edit', edit_direccion_path(direccion) %></td>
        <td><%= link_to 'Destroy', direccion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Direccion', new_direccion_path %>
